{
    "name": "Google Snapshot",
    "description": "View the news according to current Google trends",
    "version": "1.0",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "GoogleAtAGlance.html"
    },
    "host_permissions": [
        "https://trends.google.com/trends/api/*"
    ]
}